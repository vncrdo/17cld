netdiscover -i -r eth0 192.168.158.128/24

nc -vvn -z 192.168.158.129 20-30

descobrir o nome do servidor

nc -vv 192.168.158.129 80 # depois de conectar escrever GET / HTTP/1.1

echo -en "GET / HTTP/1.0\n\n\n"|nc 192.168.158.129 80 |grep Server

nc -lvp 9432 # deixar uma porta aperta
netstat -nat # valida que a porta esta aberta

nc 127.0.0.1 9432

para abrir o arquivo 

rodar o comando notepad.exe sim.txt:nao.txt


